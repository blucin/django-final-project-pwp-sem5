{
  "builds": [{
    "src": "project/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb" , "runtime": "python3.11" }
  }],
  "routes": [
    { "src": "/static/(.*)", "dest": "static/$1" },
    { "src": "/(.*)", "dest": "project/wsgi.py" }
  ],
  "static": {
    "directory": "static"
  }
}